input,textarea{display:block;font-family:"Gotham SSm A","Gotham SSm B","Helvetica Neue",Helvetica,Calibri,Arial,clean,sans-serif;font-size:13px;font-weight:400}
.fieldspan{clear:both;position:relative}
.fieldspan:before,.fieldspan:after{content:" ";display:table}
.fieldspan:after{clear:both}
input[type="text"],input[type="tel"],input[type="email"],input[type="password"]{background:#eee;border:2px solid #eee;border-radius:2px;box-sizing:border-box;height:36px;margin:0;padding:0;text-indent:7px;width:100%;-webkit-appearance:none}
