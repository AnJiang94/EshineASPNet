.custom_dropdown.three_quarters{width:74% !important}
.customSelect{background-color:#eee;border:2px solid #eee;border-radius:2px;box-sizing:border-box;-moz-box-sizing:border-box;color:#000;font-family:"Gotham SSm A","Gotham SSm B","Helvetica Neue",Helvetica,Calibri,Arial,clean,sans-serif;font-size:12px;font-weight:400;height:36px;line-height:32px;overflow:hidden;position:relative;text-indent:5px;width:100%;z-index:1}
