@media handheld, only screen and (max-width: 768px){body{color:#222;font-family:"Gotham SSm A","Gotham SSm B","Helvetica Neue",Helvetica,Calibri,Arial,clean,sans-serif;font-size:14px;line-height:22px;text-rendering:optimizeLegibility}
h1,h2,h3,h4{font-family:"Gotham SSm A","Gotham SSm B","Helvetica Neue",Helvetica,Calibri,Arial,clean,sans-serif;font-weight:normal}
h1 a,h2 a,h3 a,h4 a{color:inherit}
h1{font-size:30px;font-weight:500;line-height:28px}
h2{font-size:30px;font-weight:500;line-height:28px}
h3{font-size:20px;font-weight:400;line-height:24px;margin-bottom:10px}
h4{color:#666;font-size:18px;font-weight:400;line-height:24px}
p{color:#666;font-family:"Gotham SSm A","Gotham SSm B","Helvetica Neue",Helvetica,Calibri,Arial,clean,sans-serif;font-size:14px;line-height:22px;margin-bottom:18px}
a,a:active,a:visited{color:#999;cursor:pointer;text-decoration:none}
a:hover,a:active,a:active:hover,a:active:active,a:visited:hover,a:visited:active{color:#2e3c52}
body,html{height:100%}
body{background:#fafafa}
hr{background:#ddd;border:0;height:1px}
