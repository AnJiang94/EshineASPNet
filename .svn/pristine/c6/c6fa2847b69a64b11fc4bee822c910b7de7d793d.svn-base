#cboxWrapper,#colorbox{overflow:visible !important}
.button{border:2px solid;border-radius:2px;cursor:pointer;display:inline-block;font-weight:600;font-size:12px;font-family:"Gotham SSm A","Gotham SSm B","Helvetica Neue",Helvetica,Calibri,Arial,clean,sans-serif;line-height:22px;padding:5px 8px;text-align:center;text-transform:none;-webkit-appearance:none;-webkit-font-smoothing:antialiased}
.blue_button,a.blue_button{background:#6c98cf;border-color:#6c98cf;color:#fff}
.blue_button:hover,a.blue_button:hover{background:#384862;border-color:#384862;color:#fff}
.blue_button:active,a.blue_button:active{background:#2e3c52;border-color:#2e3c52;color:#fff}
.red_button,a.red_button{background:#dc6e65;border-color:#dc6e65;color:#fff}
.red_button:hover,a.red_button:hover{background:#d75a50;border-color:#d75a50;color:#fff}
.red_button:active,a.red_button:active{background:#d3473b;border-color:#d3473b;color:#fff}
.white_button,a.white_button{background:#fff;border-color:#fff;color:#000;text-transform:uppercase;letter-spacing:1px}
.gray_button,a.gray_button{background:#ccc;border-color:#ccc;color:#fff}
.gray_button:hover,a.gray_button:hover{background:#bfbfbf;border-color:#bfbfbf;color:#fff}
.gray_button:active,a.gray_button:active{background:#b3b3b3;border-color:#b3b3b3;color:#fff}
.outline_button,a.outline_button{border-color:#fff;color:#fff;letter-spacing:1px;text-transform:uppercase;padding:8px 12px}
