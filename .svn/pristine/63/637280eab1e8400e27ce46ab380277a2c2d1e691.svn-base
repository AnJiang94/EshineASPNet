}

.fx_banner_next {
    position: absolute;
    top: 50%;
    right: 0;
    margin-top: -315px;
    width: 41px;
    height: 119px;
    cursor: pointer;
    background: url(/icson/img/faxian/index/ban_next_24.png);
    opacity: .5;
    filter: alpha(opacity=50);
}

.ie6 .fx_banner_next, .ie6 .fx_banner_next:hover {
