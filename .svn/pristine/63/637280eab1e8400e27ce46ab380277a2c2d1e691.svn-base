    filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='//static.gtimg.com/icson/img/faxian/index/ban_next_24.png',sizingMethod='scale');
}

.fx_banner_pre:hover, .fx_banner_next:hover {
