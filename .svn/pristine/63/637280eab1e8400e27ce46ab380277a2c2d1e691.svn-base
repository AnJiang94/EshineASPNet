.fx_grid_c1 {
    width: 990px;
    margin: 0 auto;
}

.fx_mod_ib {
    display: inline-block;
    *display: inline;
    *zoom: 1;
    vertical-align: middle;
}

.fx_mod_seo {
    position: absolute;
    top: -9999em;
    left: -9999em;
}

.fx_mod_pr {
    position: relative;
}

body .show {
    display: block;
}

.hidden {
    display: none;
}

a:hover {
    color: #FF7F02;
}

a.fx_lk_1 {
    color: #FF7F02;
    text-decoration: none;
}

    a.fx_lk_1:hover {
        color: #FF7F02;
        text-decoration: underline;
    }

.fx_por {
    position: relative;
    z-index: 10;
}

.fx_tx1 {
    color: #ff7f02;
}

.fx_foot {
    margin-top: 20px;
}

    .fx_foot iframe {
        height: 390px;
        width: 100%;
        border: 0 none;
    }

.fx_mod_load {
    text-align: center;
    background: #fff url(/icson/img/faxian/common/loading.gif) no-repeat center center;
    height: 50px;
    margin: 20px auto;
}

.fx_mod_btn {
    background-image: url(/icson/img/faxian/common/faxian.png);
    background-repeat: no-repeat;
}

.fx_mod_pinfo {
    width: 95px;
    height: 50px;
    position: absolute;
    top: 15px;
    left: 0;
    z-index: 1;
    font-family: \5fae\8f6f\96c5\9ed1;
    color: #666;
    background: #fff;
}

    .fx_mod_pinfo:hover {
        background: #e8e8e8;
        color: #fc6b00;
        text-decoration: none;
    }

    .fx_mod_pinfo span {
        display: block;
        margin-left: 20px;
    }

    .fx_mod_pinfo .info {
        padding-top: 6px;
        font-size: 14px;
    }

.new_mod_action {
    font-family: "\5B8B\4F53";
}

.new_mod_action_item {
    position: relative;
    z-index: 20;
    float: left;
    padding-right: 20px;
}

.new_mod_action_discuss, .new_mod_action_share {
    display: block;
    padding-left: 25px;
    background-image: url(/icson/img/hao/bg_control.png);
    background-repeat: no-repeat;
    vertical-align: middle;
    line-height: 21px;
}

.new_mod_action_discuss {
    background-position: -145px -88px;
}

    .new_mod_action_discuss:hover {
        background-position: -145px -118px;
    }

.new_mod_action_share {
    background-position: -145px -28px;
}

    .new_mod_action_share:hover {
        background-position: -145px -58px;
    }

.new_mod_action_ding {
    overflow: hidden;
    display: inline-block;
    padding-left: 25px;
    height: 21px;
    line-height: 21px;
    background: url(/Images/bg_control.png) no-repeat -145px -208px;
    vertical-align: middle;
}

    .new_mod_action_ding:hover {
        background-position: -145px -277px;
    }

.new_mod_action_ding_on {
    cursor: default;
    background-position: -145px -277px !important;
    transition: all 800ms ease-in-out;
}

    .new_mod_action_ding_on:hover {
        cursor: default;
        text-decoration: none;
        color: #000;
    }

.new_mod_popshare {
    position: absolute;
    left: -32px;
    top: 30px;
    width: 120px;
    padding: 5px;
    border: 1px solid #CCC;
    box-shadow: 2px 2px 2px rgba(0,0,0,.1);
    background-color: #FFF;
}

    .new_mod_popshare li {
        display: inline;
        float: left;
        width: 100%;
    }

    .new_mod_popshare i {
        overflow: hidden;
        position: absolute;
        left: 50%;
        top: -16px;
        margin-left: -8px;
        width: 0;
        height: 0;
        border-width: 8px;
        border-color: transparent transparent #CCC transparent;
        border-style: dashed dashed solid dashed;
        font-size: 0;
    }

    .new_mod_popshare a {
        display: block;
        padding: 5px 10px 5px 22px;
        background-image: url(/icson/img/faxian/common/bg_control.png);
        background-repeat: no-repeat;
    }

    .new_mod_popshare .new_ico_wb {
        background-position: -145px -141px;
    }

    .new_mod_popshare .new_ico_qzone {
        background-position: -145px -173px;
    }

.fx_mod_btn {
    display: inline-block;
    *display: inline;
    *zoom: 1;
    vertical-align: middle;
    width: 129px;
    padding: 3px 0;
    _padding: 4px 0 2px 0;
    color: #fff;
    background-position: 0 0;
    font-size: 18px;
    font-family: "hiragino sans gb","\5FAE\8F6F\96C5\9ED1";
    cursor: pointer;
}

    .fx_mod_btn span {
        display: inline-block;
        *display: inline;
        *zoom: 1;
        width: 96px;
        text-align: center;
    }

    .fx_mod_btn:hover {
        color: #fff;
        text-decoration: none;
        background-position: 0 -34px;
    }

.fx_mod_tabs {
    margin: 0 0 20px 5px;
}

    .fx_mod_tabs a {
        display: inline-block;
        *display: inline;
        *zoom: 1;
        vertical-align: middle;
        position: relative;
        width: 86px;
        height: 28px;
        line-height: 28px;
        font-size: 14px;
        font-weight: 700;
        color: #626262;
        text-align: center;
        margin: 0 7px;
        font-family: "hiragino sans gb","\5FAE\8F6F\96C5\9ED1";
    }

        .fx_mod_tabs a:hover, .fx_mod_tabs .selected {
            text-decoration: none;
            color: #fff;
            background: #FF7F02;
        }

    .fx_mod_tabs i {
        display: none;
    }

    .fx_mod_tabs .selected i {
        display: block;
        width: 20px;
        height: 20px;
        position: absolute;
        bottom: -10px;
        left: 38px;
        font: 400 20px/20px simsun;
        color: #FF7F02;
    }

.fx_mod_hasgoods {
    float: right;
    margin-right: 15px;
}

    .fx_mod_hasgoods input {
        margin-right: 3px;
        position: relative;
        top: 2px;
        _top: 0;
        cursor: pointer;
    }

    .fx_mod_hasgoods label {
        cursor: pointer;
        font-family: "hiragino sans gb","\5FAE\8F6F\96C5\9ED1";
        font-size: 14px;
        color: #626262;
        font-weight: 700;
    }

.fx_mod_prolist {
    font-size: 0;
    letter-spacing: -4px;
}

.fx_mod_proitem {
    width: 485px;
    font-size: 12px;
    letter-spacing: 0;
    position: relative;
    background: #fff;
    margin: 0 5px 20px 5px;
    display: inline-block;
    *display: inline;
    *zoom: 1;
    vertical-align: top;
    transition: all 200ms ease-in-out;
}

    .fx_mod_proitem_hover, .fx_mod_proitem:hover {
        z-index: 10;
        box-shadow: 0 10px 17px rgba(110,110,110,.7);
    }

.fx_mod_proitem_img {
    margin: 15px 15px 0 15px;
    display: block;
    position: relative;
}

    .fx_mod_proitem_img img {
        width: 100%;
        display: block;
    }

    .fx_mod_proitem_img b {
        position: absolute;
        width: 100%;
        height: 100%;
        font-size: 0;
        top: 0;
        left: 0;
        opacity: 0;
        filter: alpha(opacity=0);
        transition: all 200ms ease-in-out;
    }

.fx_mod_proitem_hover .fx_mod_proitem_img b, .fx_mod_proitem:hover .fx_mod_proitem_img b {
    opacity: 1;
    filter: progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#33FFFFFF',endColorstr='#33FFFFFF');
    background-color: rgba(255,255,255,.2);
}

.ie9 .fx_mod_proitem_img b {
    filter: none !important;
}

.fx_mod_proitem_time {
    position: absolute;
    top: 15px;
    right: 15px;
    text-align: center;
    width: 103px;
    height: 22px;
    line-height: 22px;
    background: #fff;
}

.fx_mod_proitem_tit {
    padding: 0 15px;
    height: 45px;
    line-height: 45px;
    overflow: hidden;
}

    .fx_mod_proitem_tit h2, .fx_mod_proitem_tit h3,.fx_mod_proitem_tit h4 {
        font-size: 17px;
        font-family: "hiragino sans gb","\5FAE\8F6F\96C5\9ED1";
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
        margin-right: 120px;
    }

    .fx_mod_proitem_tit p {
        float: right;
        color: #FF8F44;
        font-weight: 700;
        font-size: 16px;
        width: 105px;
        text-align: right;
    }

        .fx_mod_proitem_tit p span {
            font-family: "Arial";
        }

    .fx_mod_proitem_tit a {
        color: #333;
        transition: all 400ms ease-in-out;
    }

        .fx_mod_proitem_tit a:hover {
            color: #FF8F44;
        }

    .fx_mod_proitem_tit .fx_mod_proitem_txt {
        font-family: "hiragino sans gb","\5FAE\8F6F\96C5\9ED1";
        font-size: 14px;
        color: #777;
        font-weight: 400;
    }

    .fx_mod_proitem_tit .fx_tx1 {
        color: #ff7f02;
    }

.fx_mod_proitem_cont {
    line-height: 19px;
    padding: 0 15px 5px 15px;
    height: 53px;
    overflow: hidden;
}

    .fx_mod_proitem_cont a {
        color: #777;
    }

        .fx_mod_proitem_cont a:hover {
            color: #FF7F02;
        }

.fx_mod_proitem_view {
    border-top: 1px solid #E8E8E8;
    padding: 14px 15px;
}

    .fx_mod_proitem_view p, .fx_mod_proitem_more {
        display: inline-block;
        *display: inline;
        *zoom: 1;
        vertical-align: middle;
    }

.fx_mod_proitem .fx_mod_share_pop {
    top: 33px;
}

.fx_mod_proitem_ico {
    position: absolute;
    right: 0;
    top: 0;
    z-index: 1;
    width: 90px;
    height: 90px;
    line-height: 999em;
    overflow: hidden;
    background: url(/icson/img/faxian/common/jiao_24.png) no-repeat;
    _background: none;
    _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='//static.gtimg.com/icson/img/faxian/common/jiao_24.png',sizingMethod='crop');
}

@media(min-width:1224px) {
    .fx_rwd .fx_grid_c1 {
        width: 990px;
    }

    .fx_rwd .fx_mod_proitem {
        width: 485px;
        margin: 0 5px 20px 5px;
    }
}

@media(min-width:1224px) and (max-width:1601px) {
    .fx_rwd .fx_grid_c1 {
        width: 1040px;
    }

    .fx_rwd .fx_mod_proitem {
        width: 500px;
        margin: 0 10px 20px 10px;
    }

    .fx_mod_tabs {
        margin-left: 10px;
    }
}

@media(min-width:1601px) {
    .fx_rwd .fx_grid_c1 {
        width: 1515px;
    }

    .fx_rwd .fx_mod_proitem {
        width: 485px;
        margin: 0 10px 20px 10px;
    }

    .fx_mod_tabs {
        margin-left: 10px;
    }

    .fx_banner .fx_banner_pre {
        left: -250px;
    }

    .fx_banner .fx_banner_next {
        right: -250px;
    }
}

.fx_banner {
