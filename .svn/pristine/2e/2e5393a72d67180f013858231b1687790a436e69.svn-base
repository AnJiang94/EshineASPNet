body {
    padding: 0;
    margin: 0;
    font-family: sans-serif;
    font-size: 12px;
    line-height: 1.5;
    color: #555;
    background: #fff;
}

.tab-link img {
    width: 44px;
}

.hide {
    display: none;
}

.blue {
    color: #007aff;
}

.pid-selected {
    background: #7cc8f5;
    color: #fff;
}

a,
a:hover,
a:focus,
a:active,
a.active, .aClick {
    outline: none;
    text-decoration: none;
    color: #007aff;
}

.left span {
    min-width: 45px;
}

.right span {
    max-width: 70px;
    margin-left: 15px;
    float: right;
    /*margin-left: -10px;*/
}

/* Welcome */
.welcomescreen-container {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: #fff;
    z-index: 10500;
    color: #666 !important;
    font-size: 16px;
}

.welcomescreen-closebtn {
    top: 5px;
    right: 5px;
    position: absolute;
    text-align: right;
    z-index: 9998;
    font-size: 14px;
    padding: 15px;
}

.welcomescreen-swiper {
    position: relative;
    width: 100%;
    height: 100%;
}

.welcomescreen-picture {
    width: 100%;
    text-align: center;
}

    .welcomescreen-picture img {
        width: 100%;
        height: 100%;
    }

.welcomescreen-text {
    position: absolute;
    bottom: 65px;
    left: 0;
    right: 0;
    padding-left: 20px;
    padding-right: 20px;
    text-align: center;
}

.welcomescreen-content {
    padding: 15px;
}

.welcomescreen-container .swiper-pagination-bullet {
    background: #0284b6 !important;
}

.welcomescreen-container .swiper-pagination-bullet-active {
    background: #fff;
}

.swiper-container-horizontal > .swiper-pagination {
    bottom: 20px;
}

html.with-statusbar-overlay .welcomescreen-container {
    top: 20px;
}

.navbar-inner .left.aClick {
    height: 48px;
}

.group-item {
    background: #ffffff;
    border-top: 1px solid gainsboro;
    border-bottom: 1px solid gainsboro;
    margin-bottom: 6px;
}

    .group-item .row-item {
        padding: 15px;
        overflow: hidden;
        display: block;
        border-bottom: 1px solid #dddddd;
    }

        .group-item .row-item:last-child {
            border-bottom: 0;
        }

        .group-item .row-item .row-item-dropdown {
            overflow: visible;
            color: #a4a4a4;
        }

        .group-item .row-item .form-control {
            padding-left: 0;
        }

            .group-item .row-item .form-control .edit {
                border: none;
                width: 90%;
                height: 20px;
                float: left;
            }

            .group-item .row-item .form-control .textarea {
                width: 100%;
                height: 105px;
            }

.head {
    line-height: 100px;
}

.headimg {
    width: 100px;
    height: 100px;
    border-radius: 60px;
    display: inline-block;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    background-size: cover;
    -o-background-size: cover;
    margin-right: 10px;
}

.noheadimg {
}

.group-item .row-item .form-control.edit {
    border: none;
    width: 90%;
    height: 20px;
    float: left;
}

/* home page */
.home-page-wrap #slides {
    display: none;
    position: relative;
}

    .home-page-wrap #slides .slidesjs-control {
        height: 150px;
    }

        .home-page-wrap #slides .slidesjs-control .banner {
            height: 100%;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            background-size: cover;
            -o-background-size: cover;
            background-position: center center;
            background-repeat: no-repeat;
        }

            .home-page-wrap #slides .slidesjs-control .banner.one {
                background-image: url(../img/banner/banner1.png);
            }

            .home-page-wrap #slides .slidesjs-control .banner.two {
                background-image: url(../img/banner/banner2.png);
            }

            .home-page-wrap #slides .slidesjs-control .banner.three {
                background-image: url(../img/banner/banner3.png);
            }

    .home-page-wrap #slides .slidesjs-pagination {
        list-style: none;
        position: absolute;
        margin: 0 auto;
        top: 110px;
        z-index: 100;
        width: 100%;
        text-align: center;
        padding: 0;
    }

        .home-page-wrap #slides .slidesjs-pagination li {
            margin: 0 2px;
            text-align: center;
            display: inline-block;
        }

            .home-page-wrap #slides .slidesjs-pagination li a {
                display: block;
                width: 8px;
                height: 8px;
                border-radius: 4px;
                background: rgba(255, 255, 255, 0.5);
                padding-top: 8px;
                float: left;
                overflow: hidden;
                font-size: 12px;
            }

                .home-page-wrap #slides .slidesjs-pagination li a.active,
                .home-page-wrap #slides .slidesjs-pagination li a:hover.active {
                    background: rgba(255, 255, 255, 0.8);
                    width: 10px;
                    height: 10px;
                    border-radius: 5px;
                    padding-top: 10px;
                    position: relative;
                    top: 1px;
                }

.home-page-module {
    text-align: center;
}

    .home-page-module .module {
        margin-top: 15px;
        margin-bottom: 15px;
    }

    .home-page-module .container-fluid .row p {
        font-size: 20px;
    }

    .home-page-module .container-fluid .row img {
        max-width: 49%;
    }

