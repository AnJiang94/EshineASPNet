/* ChooseAssessment */
.CAssessment-wrap,
.CIndex-wrap {
    padding: 1px;
}

    .CAssessment-wrap .row li,
    CIndex-wrap .row li {
        padding: 0px;
    }

    .CAssessment-wrap .tab-content,
    .CIndex-wrap .tab-content {
        background-color: #fff;
    }

        .CIndex-wrap .tab-content .tab.active .list-block {
            margin: 0px;
        }

            .CIndex-wrap .tab-content .tab.active .list-block ul:before {
                height: auto;
            }

            .CIndex-wrap .tab-content .tab.active .list-block ul:after {
                height: auto;
            }

        .CIndex-wrap .tab-content .tab-pane .buttons-row {
            display: block;
            list-style: none;
            -webkit-padding-start: 0px;
        }

            .CIndex-wrap .tab-content .tab-pane .buttons-row li {
                font-size: 15px;
                text-align: center;
            }

        .CAssessment-wrap .tab-content .group-item,
        .CIndex-wrap .tab-content .group-item {
            border: none;
        }

    .CAssessment-wrap .nav.nav-tabs.row li a,
    .CIndex-wrap .nav.nav-tabs.row li a {
        color: #555;
        min-width: 80px;
        text-align: center;
    }

    .CAssessment-wrap .nav.nav-tabs.row li.active > a,
    .CIndex-wrap .nav.nav-tabs.row li.active > a {
        border: 1px solid transparent !important;
        background-color: transparent !important;
        border-bottom: 2px solid #007aff !important;
        color: #007aff;
    }

    /* StartAssessment */
    .SAssessment-wrap img, .CAssessment-wrap img {
        width: 100%;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        background-size: cover;
        -o-background-size: cover;
        background-position: center center;
        background-repeat: no-repeat;
    }

.SAssessment-wrap .center {
    margin-top: 30px;
    width: 100%;
    text-align: center;
}

