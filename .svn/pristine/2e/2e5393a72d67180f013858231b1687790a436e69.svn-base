/* User */
.ULogin-wrap .group-item {
    margin-top: 35px;
    margin-bottom: 20px;
    background: none;
    border-top: none;
    border-bottom: none;
}

    .ULogin-wrap .group-item .row-item {
        border-bottom: none;
        text-align: center;
    }

        .ULogin-wrap .group-item .row-item .form-control {
            border-top: none;
            border-left: none;
            border-right: none;
