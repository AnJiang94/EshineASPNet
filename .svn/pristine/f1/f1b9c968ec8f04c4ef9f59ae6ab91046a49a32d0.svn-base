        function cidInfo(sId) {
            var iSum = 0
            var info = ""
            if (!/^\d{17}(\d|X|x)$/.test(sId)) {
                alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertneedfullid %>"></asp:Localize>');
            //TextBox3.focus();
            return;
        }
        sId = sId.replace(/x$/i, "a");
        if (aCity[parseInt(sId.substr(0, 2))] == null) { alert('Error:<asp:Localize runat="server" Text="<%$ Resources:GResource,alertwrongzone %>"></asp:Localize>'); return; }
        sBirthday = sId.substr(6, 4) + "-" + Number(sId.substr(10, 2)) + "-" + Number(sId.substr(12, 2));
        var d = new Date(sBirthday.replace(/-/g, "/"))
        if (sBirthday != (d.getFullYear() + "-" + (d.getMonth() + 1) + "-" + d.getDate())) { alert('Error:<asp:Localize runat="server" Text="<%$ Resources:GResource,alertwrongDOB %>"></asp:Localize>'); return; }
        for (var i = 17; i >= 0; i--) iSum += (Math.pow(2, i) % 11) * parseInt(sId.charAt(17 - i), 11)
        if (iSum % 11 != 1) { alert('Error:<asp:Localize runat="server" Text="<%$ Resources:GResource,alertwrongIDnum %>"></asp:Localize>'); return; }

        var d = new Date();
        age = d.getYear() - sId.substr(6, 4);
        TextBox6.value = age;

        var dy = document.getElementById("<%=DropDownListYear.ClientID%>");
        var dm = document.getElementById("<%=DropDownListMonth.ClientID%>")
        var dd = document.getElementById("<%=DropDownListDay.ClientID%>")
        var index;
        for (var j = 0; j < dy.options.length; j++) {
            if (dy.options[j].text == sId.substr(6, 4)) {
                index = j;
                break;
            }
        }
        if (index > 0) {
            dy.options[index].selected = true;
            dm.options[Number(sId.substr(10, 2))].selected = true;
            dd.options[Number(sId.substr(12, 2))].selected = true;
        }
        var ml = document.getElementById("malelabel");
        var fl = document.getElementById("femalelabel");
        if (sId.substr(16, 1) % 2) {
            RadioButton1.checked = true;
            ml.className = "cb-enable  selected";
            fl.className = "cb-disable ";
        }
        else {
            RadioButton2.checked = true;
            ml.className = "cb-enable";
            fl.className = "cb-disable selected";
        }
    }

    function checkform1() {

        var t1 = document.getElementById("<%=TextBox1.ClientID%>");
        if (t1.value == "") {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertneedname %>"></asp:Localize>');
            t1.focus();
            return false;
        }

        var t2 = document.getElementById("<%=TextBox2.ClientID%>").value;
        if (false && t2.search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) == -1) {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,aleartcorrectemail %>"></asp:Localize>');
            return false;
        }

        var t3 = document.getElementById("<%=TextBox3.ClientID%>");
        var t5 = document.getElementById("<%=TextBox5.ClientID%>");
        if (t3.value == "" && t5.value == "") {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertneedidorpass %>"></asp:Localize>');
            t3.focus();
            return false;
        }
        else if (t5.value == "" && t3.value.length < 18) {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertidmust18 %>"></asp:Localize>');
                return false;
            }

        var reg = /^1[3|4|5|7|8][0-9]\d{4,8}$/;
        var t4 = document.getElementById("<%=TextBox4.ClientID%>");
        if (t4.value == "") {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertneedmobile %>"></asp:Localize>');
            t4.focus();
            return false;
        }
        else if (t4.value.length < 11) {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertmobilemust11 %>"></asp:Localize>');
            return false;
        }
        else if (!(reg.test(t4.value))) {
            alert('<asp:Localize runat="server" Text="<%$ Resources:GResource,alertwrongmobile %>"></asp:Localize>');
                return false;
            }

    var sy = document.getElementById("<%=DropDownListYear.ClientID%>").value;
