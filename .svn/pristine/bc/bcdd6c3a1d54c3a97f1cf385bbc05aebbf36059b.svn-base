using System;
using System.Collections.Generic;
using System.Text;

namespace Model
{
    public class tab_message
    {
        private int _Messageid = 0;
        private string _Sender = "";
        private string _Receiver = "";
        private string _Title = "";
        private string _Msg = "";
        private string _Status = "";
        private DateTime _MessageDate = DateTime.Parse("1900-01-01");
        private int _MCommonid = 0;


        public int Messageid
        {
            get { return _Messageid; }
            set { _Messageid = value; }
        }

        public string Sender
        {
            get { return _Sender; }
            set { _Sender = value; }
        }

        public string Receiver
        {
            get { return _Receiver; }
            set { _Receiver = value; }
        }

        public string Title
        {
            get { return _Title; }
            set { _Title = value; }
        }

        public string Msg
        {
            get { return _Msg; }
            set { _Msg = value; }
        }

        public string Status
        {
            get { return _Status; }
            set { _Status = value; }
        }

        public DateTime MessageDate
        {
            get { return _MessageDate; }
            set { _MessageDate = value; }
        }

        public int MCommonid
        {
            get { return _MCommonid; }
            set { _MCommonid = value; }
        }



    }
}
