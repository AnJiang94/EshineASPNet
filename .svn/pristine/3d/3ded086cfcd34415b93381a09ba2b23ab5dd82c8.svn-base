using System;
using System.Collections.Generic;
using System.Reflection;
using System.Text;

namespace DALFactory
{
   public  class AssessmentAnswer_Factory
    {
        static readonly string path = System.Configuration.ConfigurationManager.AppSettings["DAL"];
        public static IDAL.AssessmentAnswerDal Createusers()
        {
            string classname = path + ".sql_AssessmentAnswer";
            return (IDAL.AssessmentAnswerDal)Assembly.Load(path).CreateInstance(classname);
        }
    }
}
