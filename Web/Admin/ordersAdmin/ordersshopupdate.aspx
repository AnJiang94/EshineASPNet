<%@ Page Language="C#" AutoEventWireup="true" CodeFile="ordersshopupdate.aspx.cs" Inherits="Admin_ordersAdmin_ordersshopupdate" %>


<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>订单详细信息</title>
    <link href="../../assets/trunkclub-d1b693260400bfdc2a0f7777f2fc64fc.css" media="screen" rel="stylesheet" type="text/css" />
</head>
<body>
    <form id="form1" runat="server">
        <div class='row'>
            <div class="twelvecol">
                <h2>商城订单信息</h2>
                <div class='buttons'>
                    <asp:Button ID="Button1" runat="server" class="button blue_button" OnClick="Button1_Click" Text="更　新" />
                    <input id="Button2" class="button" onclick="location = 'ordersshop.aspx'" type="button" value="返　回" />
                    
                </div>
            </div>
        </div>
        <div class="row sizes">
            <div class="sixcol" style="text-align: left;">
                <h3>
                    <asp:Label ID="Label45" runat="server" Text="订单信息" Font-Bold="True"
                        meta:resourcekey="Label45Resource1"></asp:Label></h3>
                <div class="input">
                    <asp:Label ID="Label6" runat="server" Text="订单号"></asp:Label>：
                    <asp:TextBox class="string required" Font-Size="Large" ID="TextBox5" runat="server" placeholder="订单号" Enabled="false"></asp:TextBox>
                </div>
                <div class="input">
                    <asp:Label ID="Label11" runat="server" Text="下单日期"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox31" runat="server" placeholder=""></asp:TextBox>
                </div>
                <div class="input">
                    <asp:Label ID="Label21" runat="server" Text="姓名"
                        meta:resourcekey="Label21Resource1"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox1" runat="server" placeholder=""></asp:TextBox>
                </div>
                <div class="input">
                    <asp:Label ID="Label22" runat="server" Text="公司"
                        meta:resourcekey="Label22Resource1"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox2" runat="server" placeholder=""></asp:TextBox>
                </div>
                <div class="input">
                    <asp:Label ID="Label25" runat="server" Text="金额"
                        meta:resourcekey="Label25Resource1"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox3" runat="server" placeholder=""></asp:TextBox>
                </div>
                
                
            </div>

            <div class="sixcol last" style="text-align: left;">
                <br />
                <div class="input">
                    <asp:Label ID="Label27" runat="server" Text="订单内容"
                        meta:resourcekey="Label27Resource1"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox4" runat="server" placeholder="" TextMode="MultiLine" Height="116" Enabled="false"></asp:TextBox>
                </div>
                <div class="input">
                    <asp:Label ID="Label29" runat="server" Text="订单状态"
                        meta:resourcekey="Label29Resource1"></asp:Label>：
                    <asp:DropDownList ID="DropDownList2" runat="server" AutoPostBack="True" class="select required customSelect">
                        <asp:ListItem>选择订单状态</asp:ListItem>
                        
                        <asp:ListItem>待付款</asp:ListItem>
                        <asp:ListItem>付款完成</asp:ListItem>
                        <asp:ListItem>已取消</asp:ListItem>
                    </asp:DropDownList>
                </div>
                
                <div class="input">
                    <asp:Label ID="Label39" runat="server" Text="邮寄地址"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox8" runat="server" placeholder=""></asp:TextBox>
                </div>
                <div class="input">
                    <asp:Label ID="Label1" runat="server" Text="快递单号"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox6" runat="server" placeholder=""></asp:TextBox>
                </div>
                
                <div class="input">
                    <asp:Label ID="Label9" runat="server" Text="备注"></asp:Label>：
                    <asp:TextBox class="string required" ID="TextBox25" runat="server" placeholder=""></asp:TextBox>
                </div>

            </div>
        </div>

    </form>
</body>
</html>
